{
  "manifest_version": 3,
  "name": "Tab Stash",
  "version": "1.0.0",
  "description": "Quickly save and restore your incognito tabs with one click",
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      
      
      "128": "icons/naughty.png"
    }
  },
  "icons": {
  "128": "icons/naughty.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "incognito": "spanning"
}


